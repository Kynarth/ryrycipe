<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="138.0" prefWidth="210.0" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ryrycipe.controller.MaterialNumberDialogController">
    <ImageView fx:id="materialImg" fitHeight="40.0" fitWidth="40.0" layoutX="39.0" layoutY="35.0" pickOnBounds="true" preserveRatio="true" />
    <ButtonBar layoutX="375.0" layoutY="331.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
        <buttons>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOKBtn" text="%button.ok.text" />
            <Button mnemonicParsing="false" onAction="#handleCancelBtn" text="%button.cancel.text" />
        </buttons>
    </ButtonBar>
    <TextField fx:id="nbMaterialField" layoutX="105.0" layoutY="42.0" onKeyPressed="#handleEnterPressed" prefHeight="26.0" prefWidth="85.0" AnchorPane.rightAnchor="20.0" />
</AnchorPane>
